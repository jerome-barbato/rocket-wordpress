{
    "name":"metabolism/rocket-wordpress",
    "version":"1.1.0",
    "description": "Wordpress Scaffolder using Rocket Framework",
    "config": {
        "preferred-install": "dist"
    },
    "repositories": [
        { "type": "composer", "url": "https://wpackagist.org"},
        { "type": "gitlab",   "url": "https://git.metabolism.fr/rocket/wordpress" },
        { "type": "package",  "package": { "name": "elliotcondon/advanced-custom-fields-pro", "version": "5.5.5", "type": "wordpress-plugin", "dist": { "type": "zip", "url": "https://connect.advancedcustomfields.com/index.php?p=pro&a=download&k=b3JkZXJfaWQ9NTgyNTZ8dHlwZT1kZXZlbG9wZXJ8ZGF0ZT0yMDE1LTA2LTE3IDEwOjA3OjI2" } } }
    ],
    "require": {
        "php" : ">=5.5.0",
        "johnpbloch/wordpress-core": "*",
        "mustangostang/spyc": "^0.6.1",
        "symfony/routing":"^3.2",
        "dflydev/dot-access-data": "^1.0",
        "wpackagist-plugin/wordpress-seo": "*",
        "wpackagist-plugin/yoast-seo-acf-analysis": "*",
        "wpackagist-plugin/cache-enabler": "*",
        "wpackagist-plugin/timber-library": "*",
        "elliotcondon/advanced-custom-fields-pro": "*"
    },
    "extra": {
        "file-management": {
            "create": {
                "metabolism/rocket-wordpress": {
                    "app/cms/uploads": 777,
                    "app/resources/acf": 777
                }
            },
            "symlink": {
                "metabolism/rocket-wordpress": {
                    "wp-config.php": "web/edition/wp-config.php",
                    "src/theme": "app/cms/themes/rocket",
                    "doc": "doc/wordpress",
                    "src/Helper/Rocket.php": "app/cms/mu-plugins/Rocket.php",
                    "../../../app/cms/uploads": "web/upload"
                }
            },
            "copy": {
            },
            "remove": {
                "metabolism/rocket-wordpress": [
                    "web/edition/wp-content",
                    "app/config/silex.yml",
                    "web/edition/wp-config-sample.php"
                ]
            }
        },
        "branch-alias": {
            "dev-dev": "1.1.x-dev"
        }
    },
    "autoload"    : {
        "psr-4" : {
            "Rocket\\" : "src/"
        }
    }
}
