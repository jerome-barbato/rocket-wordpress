{
    "name":"metabolism/rocket-wordpress",
    "version":"3.0.0",
    "repositories": [
        {
            "type": "vcs",
            "url": "ssh://git@git.metabolism.fr/rocket/wordpress.git"
        }
    ],
    "require": {
        "php" : ">=5.6.0",
        "johnpbloch/wordpress": "^4.6",
        "mustangostang/spyc": "^0.6.1",
        "dflydev/dot-access-data": "^1.0"
    },
    "extra": {
        "installer-paths": {
            "var/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
            "var/plugins/{$name}/": ["type:wordpress-plugin"]
        },
        "wordpress-install-dir": "vendor/wordpress"
    },
    "autoload"    : {
        "psr-4" : {
            "Rocket\\" : "src/"
        }
    }
}