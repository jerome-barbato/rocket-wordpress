{
    "name":"metabolism/rocket-wordpress",
    "license": "AGPL-3.0",
  "version": "1.2.0",
    "description": "Wordpress Scaffolder using Rocket Framework",
    "config": {
        "preferred-install": "dist"
    },
    "require": {
        "php" : ">=5.5.0",
        "johnpbloch/wordpress-core": "*",
        "mustangostang/spyc": "^0.6.1",
        "symfony/routing":"^3.2",
        "dflydev/dot-access-data": "^1.0",
        "wpackagist-plugin/cache-enabler": "*",
        "wpackagist-plugin/timber-library": "*",
        "elliotcondon/advanced-custom-fields-pro": "5.*",
        "wpackagist-plugin/post-types-order": "*"
    },
    "suggest": {
      "metabolism/rocket-kernel": "*"
    },
    "extra": {
        "file-management": {
            "create": {
                "metabolism/rocket-wordpress": {
                    "web/app/uploads": 777,
                    "app/resources/acf": 777
                }
            },
            "symlink": {
                "metabolism/rocket-wordpress": {
                    "../../johnpbloch/wordpress-core": "web/edition",
                    "wp-config.php": "vendor/johnpbloch/wordpress-core/wp-config.php",
                    "src/theme": "web/app/themes/rocket",
                    "doc": "doc/wordpress",
                    "src/Helper/Rocket.php": "web/app/mu-plugins/Rocket.php"
                }
            },
            "copy": {
            },
            "remove": {
                "metabolism/rocket-wordpress": [
                    "web/edition/wp-content",
                    "app/config/silex.yml",
                    "web/edition/wp-config-sample.php"
                ]
            }
        },
        "branch-alias": {
            "dev-dev": "1.2.x-dev"
        }
    },
    "autoload"    : {
        "psr-4" : {
            "Rocket\\" : "src/"
        }
    }
}
